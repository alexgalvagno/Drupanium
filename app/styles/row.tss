"#row": {
	height: '50dp',
    className: 'itemRow'
}

"#title": {
	height: Titanium.UI.FILL,
    font: {
        fontSize: '16dp',
        fontWeight: 'bold'
    },
    color:'#000000',
    left: '10dp',
    top:'0',
    touchEnabled: false
}

"#title[platform=ios]": {
	font: {
        fontSize: '14dp'
    }
}

"#image": {
	defaultImage: '/images/wait.png',
	height: Titanium.UI.SIZE,
	top: 0
}